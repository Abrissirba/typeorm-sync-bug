{
    "type": "mssql",
    "host": "HOST",
    "username": "USERNAME",
    "password": "PASSOWORD",
    "database": "DATABASE",
    "synchronize": true,
    "logging": ["error"],
    "options": {
         "encrypt": true
    },
    "entities": [
       "db/entity/**/*.ts"
    ],
    "migrations": [
       "db/migration/**/*.ts"
    ],
    "subscribers": [
       "db/subscriber/**/*.ts"
    ],
    "cli": {
       "entitiesDir": "db/entity",
       "migrationsDir": "db/migration",
       "subscribersDir": "db/subscriber"
    }
 }
